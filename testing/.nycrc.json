{
  "all": true,
  "include": [
    "../noesis-docs/**/*.js",
    "../platform/**/*.js"
  ],
  "exclude": [
    "../noesis-docs/_site/**",
    "../noesis-docs/.jekyll-cache/**",
    "../noesis-docs/.sass-cache/**",
    "../noesis-docs/vendor/**",
    "../noesis-docs/.bundle/**",
    "../noesis-docs/node_modules/**",
    "../platform/**/node_modules/**",
    "../platform/**/dist/**",
    "../platform/**/build/**",
    "../platform/**/coverage/**",
    "../platform/**/test/**",
    "../platform/**/tests/**",
    "../platform/**/*.test.js",
    "../platform/**/*.spec.js",
    "**/*.config.js",
    "**/webpack.config.js",
    "**/jest.config.js",
    "**/cypress.config.js",
    "scripts/**",
    "cypress/**",
    "unit/**",
    "node_modules/**",
    "coverage/**",
    ".nyc_output/**"
  ],
  "reporter": [
    "text",
    "text-summary", 
    "html",
    "lcov",
    "json"
  ],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output",
  "check-coverage": false,
  "lines": 10,
  "statements": 10,
  "functions": 10,
  "branches": 10,
  "extension": [".js"],
  "cache": true,
  "instrument": true,
  "sourceMap": false
}
